---
interface Props {
  class?: string;
  width?: string;
  height?: string;
  [key: string]: any;
}

const { class: className, width = "10", height = "7", ...props } = Astro.props;
---

<svg
  width={width}
  height={height}
  viewBox="0 0 10 7"
  fill="none"
  xmlns="http://www.w3.org/2000/svg"
  class={className}
  {...props}
>
  <path
    d="M8.05264 2L5.05264 5.00649L2.05264 2"
    stroke="#a3a6b3"
    stroke-width="1.25"
    stroke-linecap="square"
  >
  </path>
</svg>